        <form class="sso-miniprogram-settings">
            <div class="form-group">
                <label for="id">App ID</label>
                <input type="text" name="id" title="App ID" class="form-control" placeholder="App ID">
            </div>
            <div class="form-group">
                <label for="secret">App Secret</label>
